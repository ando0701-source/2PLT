{
  "kind": "2PLT_BOOT_LOADER_TRIGGER_V1",
  "trigger": "@@@@2PLT_JL_COMMIT@@@@",
  "doc_set_name": "MINSET_JL_COMMIT_V1",
  "purpose": "BOOT LOADER: load required documents by physical paths before DOC_ID resolution is available.",
  "constraints": {
    "no_discovery": true,
    "paths_are_repo_relpath_posix": true
  },
  "load": {
    "mode": "physical_path_only",
    "order": [
      "docs/2PLT_00_MODEL.md",
      "docs/2PLT_00_DOCUMENT_GOVERNANCE.md",
      "docs/2PLT_00_ENTRYPOINT.md",
      "docs/2PLT_05_DOC_ID_VOCAB.md",
      "docs/2PLT_10_STATE_MACHINE.md",
      "docs/2PLT_10_RESPONSIBILITY.md",
      "docs/2PLT_20_MANAGER_BLOCK_GRAMMAR.md",
      "docs/2PLT_20_TRIGGER_ID_VOCAB.md",
      "docs/2PLT_20_ARTIFACT_META_VOCAB.md",
      "docs/2PLT_20_OWNER_ID_VOCAB.md",
      "docs/2PLT_20_REQUEST_ID_VOCAB.md",
      "docs/2PLT_20_REASON_CODE_VOCAB.md",
      "docs/2PLT_20_DOC_SET_VOCAB.md",
      "docs/2PLT_20_OUTPUT_TEMPLATE_VOCAB.md",
      "docs/2PLT_30_TRIGGER_TERMINAL_MATRIX.md",
      "docs/2PLT_40_EXECUTION_POLICY.md",
      "docs/2PLT_40_OUTPUT_SCHEMA.md",
      "docs/2PLT_50_PROFILE_JUDGEMENT_LOG_COMMIT.md",
      "docs/2PLT_50_PROFILE_JUDGEMENT_LOG_PROPOSAL.md"
    ]
  },
  "notes": [
    "This JSON contains no DOC_ID-based rules.",
    "Loaded files correspond to the DOC_SET specified in 2PLT_00_ENTRYPOINT.md.",
    "BOOT stage does not decide terminal outcomes; REJECT may later fallback to UNRESOLVED."
  ]
}
